{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "witnessos-consciousness",
  "title": "WitnessOS Consciousness",
  "description": "Access consciousness engines and spiritual guidance through WitnessOS API",
  "icon": "ðŸŒŠ",
  "author": "witnessos",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "daily-guidance",
      "title": "Daily Guidance",
      "description": "Get personalized daily guidance from consciousness engines",
      "mode": "view"
    },
    {
      "name": "numerology-reading",
      "title": "Numerology Reading",
      "description": "Calculate your numerology profile",
      "mode": "view"
    },
    {
      "name": "tarot-reading",
      "title": "Tarot Reading",
      "description": "Get a tarot reading for your question",
      "mode": "view"
    },
    {
      "name": "biorhythm-check",
      "title": "Biorhythm Check",
      "description": "Check your current biorhythm cycles",
      "mode": "view"
    },
    {
      "name": "iching-wisdom",
      "title": "I-Ching Wisdom",
      "description": "Consult the I-Ching for guidance",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "apiToken",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "Your WitnessOS API JWT token",
      "placeholder": "Enter your JWT token from WitnessOS"
    },
    {
      "name": "apiBaseUrl",
      "type": "textfield",
      "required": false,
      "title": "API Base URL",
      "description": "WitnessOS API base URL (leave empty for production)",
      "default": "https://api.witnessos.space",
      "placeholder": "https://api.witnessos.space"
    },
    {
      "name": "fullName",
      "type": "textfield",
      "required": true,
      "title": "Full Name",
      "description": "Your full name for consciousness calculations",
      "placeholder": "John Doe"
    },
    {
      "name": "birthDate",
      "type": "textfield",
      "required": true,
      "title": "Birth Date",
      "description": "Your birth date (YYYY-MM-DD format)",
      "placeholder": "1990-01-15"
    },
    {
      "name": "birthTime",
      "type": "textfield",
      "required": false,
      "title": "Birth Time",
      "description": "Your birth time (HH:MM format, optional)",
      "placeholder": "14:30"
    },
    {
      "name": "birthLocation",
      "type": "textfield",
      "required": false,
      "title": "Birth Location",
      "description": "Your birth location (optional)",
      "placeholder": "New York, NY, USA"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}